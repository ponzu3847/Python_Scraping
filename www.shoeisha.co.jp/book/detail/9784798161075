  <link rel="stylesheet" href="/assets/css/baguetteBox.min.css">
  <script src="/assets/js/baguetteBox.min.js"></script>
  <script src="/assets/js/related_book.js"></script>
  <script>
  $(function() {
      moment.locale('ja');
      function isShowImageGallery(ua) {
        if (ua.indexOf('iPhone') > 0 || ua.indexOf('Android') > 0 && ua.indexOf('Mobile') > 0) {
            return true;
        } else if (ua.indexOf('iPad') > 0 || ua.indexOf('Android') > 0) {
            return true;
        }

        return false;
      }
      var ua = navigator.userAgent;
      if (isShowImageGallery(ua)) {
        $('.single-item').slick({
          dots: true,
          arrows:false,
          variableWidth: false,
          adaptiveHeight: true,
          lazyLoad:'ondemand',
        });

        (function() {
          baguetteBox.run('.splus-image-gallery-item');
        })();

      } else {
        $('.bookImageModal').on('click', function(e) {
          e.preventDefault();
          $('#splus-ga-modal').modal();
        });
      }

      var cxBookFormat = document.getElementById('cx-book-format');
      RelatedBookWidget.init(document.getElementById('rec-related-book-list')).bookPage.build({
        format: cxBookFormat.dataset.bookFormat,
        isbn: cxBookFormat.dataset.bookIsbn,
        genre: cxBookFormat.dataset.bookGenre,
        authors: cxBookFormat.dataset.bookAuthors,
      });
  });
     
    /** 正誤表関連スクリプト */
    (function($,w){
        var state = {
          book_type:null,
          print_num:null,//ユーザーの購入した刷数
        };
        var book_type = {
          'paper':'書籍・SEshop限定PDF',
          'epub' :'電子書籍',
        };
        var init = function(){
          state.book_type=null;
          state.print_num=null;
        };
        //正誤の内容の画像を拡大できるようにする
        var makeImageExpandable = function(){
          //errata 画像 幅384px超える場合は縮小表示しつつlightbox拡大リンクを生成する
          var img = new Image();
          $("#errata .errta-table").find('img').each(function(i, elem) {
            img.src = elem.src;
            if ($(elem).attr("width")) {
              // 明示的にwidthが設定されているときはwidthを自動的に設定しない
              if (img.width > 384) {
                $(elem).after('<div style="text-align:right"><span class="glyphicon glyphicon-zoom-in" aria-hidden="true"></span><a href="' + elem.src + '" rel="lightbox">拡大</a></div>');
              }
            } else if (img.width > 384) {
              //$(elem).attr("rel", 'lightbox');
              $(elem).attr("width", 384);
              $(elem).after('<div style="text-align:right"><span class="glyphicon glyphicon-zoom-in" aria-hidden="true"></span><a href="' + elem.src + '" rel="lightbox">拡大</a></div>');
            } else if (img.width == 0) {
              // $(document).ready(function() { 等で画像の読み込みが終わらないのにこのeachルーチンに入るとwidthが取れないケースがある
              // その場合に画像がはみ出さないようにmax-widthは設定しておく 規定サイズを上回った場合に拡大ボタンが出ない
              $(elem).css( "max-width" , "384px" );
              // console.log('case1 ' + elem + ' --- ' + img.width + ' --- ' + img.height);
            } else {
              // console.log('case2 ' + elem + ' --- ' + img.width + ' --- ' + img.height);
            }
          });
        };
        //修正されていない正誤をすべて表示する
        var showUnfixedErratas = function(){

          if(state.print_num === null){//電子書籍の場合
            $('.erra_fixed').hide();
            $('.erra_not_fixed').show();
            return $('.erra_not_fixed').length;
          }
          var shown_rows = 0;
          $('.erra-row').each(function(erra){
            //未修正の場合はfixed-atには0が入っている
            if($(this).data('fixed-at') == '0' || $(this).data('fixed-at') > state.print_num){
              $(this).show();
              shown_rows++;
            }else{
              $(this).hide();
            }
          });
          return shown_rows;
        };
        var showAllErratas = function(){
          $('.erra_fixed').show();
          $('.erra_not_fixed').show();
          return $('.erra_fixed').length + $('.erra_not_fixed').length;
        };
        var updateCurrentErrataVersionView = function(){
          $("#errata #current_type").html(book_type[state.book_type]);

          var print_num = "";
          if(state.print_num === "all"){
            print_num = "全刷";
          }else{
            print_num = state.print_num;
          }
          $("#errata #current_print_num").html(print_num);

          if(state.book_type == 'epub'){
            $("#errata .current_print_num").hide();
          }else{
            $("#errata .current_print_num").show();
          }
        }
        var showBookTypePickView = function(){
          $("#errata .picker").show();
          $("#errata .pick-booktype").show();
          $("#errata .pick-print").hide();
          $("#errata .errata-table-wrapper").hide();
        }
        var showPrintNumPickView = function(){
          $("#errata .picker").show();
          $("#errata .pick-booktype").hide();
          $("#errata .pick-print").show();
          $("#errata .errata-table-wrapper").hide();

        };
        //正誤表を表示
        var showErrataTableView = function(){
          // state.shown_rows = 0;
          var shown_rows = 0;
          if(state.book_type === 'paper' && state.print_num === 'all'){
            shown_rows = showAllErratas();
          }else{
            shown_rows = showUnfixedErratas();
          }

          updateCurrentErrataVersionView();
          $("#errata .picker").hide();
          $("#errata .errata-table-wrapper").show();

          if(shown_rows < 1){
            $("#errata .errata-table-wrapper table").hide();
            $("#errata .errata-table-wrapper .errata-not-found").show();
            $("#errata .errata-table-wrapper .errata-apology").hide();
          }else{
            $("#errata .errata-table-wrapper table").show();
            $("#errata .errata-table-wrapper .errata-not-found").hide();
            $("#errata .errata-table-wrapper .errata-apology").show();
          }
        };
        var updateView = function(){
          if(state.book_type === null && state.print_num === null){
            showBookTypePickView();
            return;
          }
          if(state.book_type === 'epub'){
            showErrataTableView();
            return;
          }
          if(state.book_type === 'paper'){
            if(state.print_num === null ){
              showPrintNumPickView();
            }
            if(state.print_num > 0 ){
              showErrataTableView();
            }
            if(state.print_num === 'all' ){
              showErrataTableView();
            }
            return;
          }
          throw new Exception("不明な状態です。");
        };
        //書籍の種類選択時
        $('#errata .book_type').on('click', function(){
          state.book_type = $(this).val();
          updateView();
        });
        //刷数の選択時(紙の書籍の場合のみ)
        $('#errata #pick_print_num').on('click', function(){
          state.print_num = $('#errata #print_num').val();
          updateView();
        });
        $('#errata .errata-init').on('click', function(){
          init();
          updateView();
        });
        makeImageExpandable();
        // leaveOnlyOneTable();

        // example hash #errata-paper-1 or #errata-paper
        var initErrataView = function(hash) {
          if (!hash) {
            return;
          }
          if (hash.indexOf('#errata-') === -1) {
            return;
          }

          $('[href="#errata"]').tab('show');

          var splited = hash.split('-');
          if (splited.length < 2) {
            return;
          }

          state.book_type = splited[1];
          state.print_num = typeof splited[2] !== 'undefined' ? splited[2] : null;
          updateView();
        };
        initErrataView(w.location.hash);
    })($,window);



  </script>


  

  <!-- TemplateEnd -->

  </body>
</html>
