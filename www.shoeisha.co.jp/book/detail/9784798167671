<script type="text/javascript">

$('[href="' + location.hash + '"]').tab('show');

$('.tabAnc').click(function (e) {
      e.preventDefault();
      $(this).tab('show');
     });

var CustomLP = {
  isOpenPreview: false,
  setScrollEvent: function(selector, position) {
    $(document).on('click', selector ,function(e){
      $('html,body').animate({
        scrollTop: position
      }, 500, 'swing');

      return false;
    });
  }
};

function cb_preview(data) {
    var isbn_main = $('#isbn_main').text();
    var isShowMain = false;
    for (var p in data) {
        var preview = data[p].preview;
        if (preview == 'partial') {
            var prev_url = data[p].preview_url;
            var isbn = p.replace('ISBN:','');
            if (isbn == isbn_main) {
                $('#pv_'+isbn).html('<a href="/book/preview/'+isbn+'/confirm" class="btn btn-primary btn-block"><span class="glyphicon glyphicon-book"></span> 立ち読み</a>');
                isShowMain = true;
            } else {
                //$('#pv_'+isbn).html('<a href="/book/preview/'+isbn+'"><span class="label label-primary"><i class="glyphicon glyphicon-book"></i> 立ち読み</span></a>');
            }
        }
    }
    if(!isShowMain)
    {
      $.get("https://www.googleapis.com/books/v1/volumes?q=isbn:"+isbn_main, function(data){
        if(typeof data.items !== "undefined" && data.items.accessInfo == "PARTIAL")
        {
          $('#pv_'+isbn_main).html('<a href="/book/preview/'+isbn_main+'/confirm" class="btn btn-primary btn-block"><span class="glyphicon glyphicon-book"></span> 立ち読み</a>');
          isShowMain = true;
        }
      });
    }
    CustomLP.isOpenPreview = isShowMain;
}

function insscript(isbnlist, callback) {
    var src_url = 'https://books.google.com/books?jscmd=viewapi&callback='+callback+'&bibkeys='+isbnlist;
    var scr = document.createElement("script");
    scr.async = true;
    scr.src = src_url;
    var s0 = document.getElementsByTagName('script')[0];
    s0.parentNode.insertBefore(scr, s0);
}

$(document).ready(function() {
    var isbnlist = jQuery.map($('.isbn'), function(n) { return 'ISBN:'+$(n).text().trim(); }).join(',');

          insscript(isbnlist, 'cb_preview');
    
    
        });
</script>
