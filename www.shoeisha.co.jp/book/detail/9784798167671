$(function(){
  function changeInputType(inputId, type) {
      var input = $("#" + inputId);
      $(input).replaceWith($('<input class="form-control" placeholder="Password" />').val(input.val()).attr("placeholder", input.attr("placeholder")).attr("id", inputId).attr("type", type).attr("name", 'password'));
  }

  $("#show-ps").change(function() {
      //チェックされてるときはtextを適応する
      if ($(this).prop('checked')) {
          changeInputType("password", "text");
      } else {
          changeInputType("password", "password");
      }
  });

  if ($("#show-ps").attr("checked")) {
      changeInputType("password", "text");
  }
});
// -->
</script>
