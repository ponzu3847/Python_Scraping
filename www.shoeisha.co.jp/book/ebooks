<script>
  $(function(){
    //if (get_view_mode() == 'pc') $('a[rel=popover]').popover();

    $('#modal_login').on('shown.bs.modal', function(e) {
      $("#email").focus();
    });

    $('#modal_login').on('hidden.bs.modal', function(e) {
      $('#valid_message').html('');
    });

    if (location.hash.indexOf('#login_e') == 0){
      $('#valid_message').html('<font color="red"><b>IDまたはパスワードが違います。</b></font>');
      $('#modal_login').modal('show')
    }
  });
</script>

        <a class="btn btn-warning" href="/book/id/?ref=/book/ebooks" role="button">会員登録（無料）</a>
      <a class="btn btn-default" data-toggle="modal" href="#modal_login">ログイン</a>
      </div>

